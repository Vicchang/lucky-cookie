{
  "manifest_version": 2,

  "name": "Lucky Cookie",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": { "scripts": ["popup.js"] },
  
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "Time information",
    "default_popup": "popup.html"
  }
}
